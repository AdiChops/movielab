doctype html
head
  include headTemplate.pug
  title Persons Page
  link(rel="stylesheet", href="/styles/resultsPage.css")

body
    include navigationTemplate.pug
    main
        input.search-bar(type='text' placeholder='Search...')
        h1 PERSONS
        .column
            each personObj in personData
                a(class="white-background", href="/persons/" + personObj.id) #{personObj.firstName} #{personObj.lastName}